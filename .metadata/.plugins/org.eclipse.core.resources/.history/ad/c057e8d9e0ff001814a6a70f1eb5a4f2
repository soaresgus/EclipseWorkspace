package me.ninjay.oneperfil.ranks;

import org.bukkit.command.Command;
import org.bukkit.command.CommandExecutor;
import org.bukkit.command.CommandSender;
import org.bukkit.entity.Player;
import org.bukkit.inventory.Inventory;

import net.eduard.api.lib.Mine;

public class ComandoRankUp implements CommandExecutor{
	
	@Override
	public boolean onCommand(CommandSender sender, Command cmd, String lb, String[] args) {
		if(sender instanceof Player) {
			Player p = (Player) sender;
			Inventory rank1i = Mine.newInventory("Cold III", 9*3);
			Inventory rank2i = Mine.newInventory("Cold II", 9*3);
			Inventory rank3i = Mine.newInventory("Cold I", 9*3);
			
			Inventory rank4i = Mine.newInventory("Dark III", 9*3);
			Inventory rank5i = Mine.newInventory("Dark II", 9*3);
			Inventory rank6i = Mine.newInventory("Dark I", 9*3);
			
			Inventory rank7i = Mine.newInventory("Flight III", 9*3);
			Inventory rank8i = Mine.newInventory("Flight II", 9*3);
			Inventory rank9i = Mine.newInventory("Flight I", 9*3);
			
			Inventory rank10i = Mine.newInventory("Souls III", 9*3);
		}
		return false;
	}

}
